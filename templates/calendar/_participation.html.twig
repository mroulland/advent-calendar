{% if already_done %}
    <p class="already-done">Tu as déjà participé à ce défi, rendez-vous au suivant !</p>
    
{% else %}

    {{ form_start(form,{ 'attr': { 'data-turbo': 'false' }}) }}
    {{ form_widget(form) }}
    
    {% if app.user %}
        <button type="submit" class="btn submit">Envoyer vos réponses</button>
    {% endif %}
    
    {{ form_end(form) }}

{% endif %}
